{
  "rules": {
    "no-extra-parens": 0,
    "react/jsx-uses-vars": 1,
    "quotes":[1,"single"],
    "strict": 0,
    "key-spacing": 0
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "node": false,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
      "eslint-plugin-react"
  ]
}